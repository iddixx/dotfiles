{
   "rpc.detailsEditing": "In {workspace_folder}"
  ,"rpc.showProblems": false
  ,"rpc.lowerDetailsEditing": "Dirtying {file_name}"
  ,"rpc.problemsText": " "
  ,"rpc.detailsViewing": "In {workspace_folder}"
  ,"rpc.lowerDetailsViewing": "Eyeballing {file_name}"
  ,"rpc.lowerDetailsNotFound": "untitled folder"
  ,"rpc.workspaceElapsedTime": false
  ,"diagnostic.enable": true
  ,"clangd.arguments": ["--background-index"]
  ,"clangd.fallbackFlags": [ "-std=c++23" ]
  ,"clangd.disableDiagnostics": true
  ,"inlayHint.enable": false
  ,"languageserver": {
      "ada": {
          "settings": {
              "ada": {
                  "projectFile": "gnat/vss_text.gpr"
                 ,"sourceInfoDiagnostics": false
              }
          },
          "command": "$HOME/.local/bin/ada_language_server",
          "filetypes": [
              "ads",
              "adb",
              "ada"
          ]
      }
  }
}
